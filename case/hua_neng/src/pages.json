    {
      "path": "pages/points/index",
      "type": "page",
      "layout": "tabbar",
      "style": {
        "navigationBarTitleText": "",
        "navigationBarBackgroundColor": "#1A65EE"
      },
      "name": "points"
    },
    {
      "path": "pages/points/pointsRedemption",
      "type": "page",
      "style": {
        "navigationBarTitleText": "积分兑换",
        "navigationBarBackgroundColor": "#FFFFFF"
      },
      "name": "myIndex"
    }
